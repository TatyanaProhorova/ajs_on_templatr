{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  // "parserOptions": {
  //   "sourceType": "module"
  // },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "never"
      }
   ]
  }
}
